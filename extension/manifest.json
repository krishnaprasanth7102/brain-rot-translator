{
  "manifest_version": 3,
  "name": "Brain Rot Translator",
  "version": "1.0",
  "description": "Automatically translates brain rot text to normal English",
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
